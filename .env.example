# Fichier d'environnement exemple
# URL du serveur Redis (partagé entre tous les services)
REDIS_URL=redis://redis:6379

# URL du modèle IA principal (roi_per_sec)
AI_MODEL_URL=http://ai_model:8000/predict

# URL du modèle IA secondaire (exit_predictor)
EXIT_MODEL_URL=http://ai_model:8000/exit

# Activer les trades réels (true = live, false = simulation)
LIVE_MODE=false

# Clé de wallet utilisée pour les swaps sur Solana
WALLET_KEYPAIR_PATH=wallet/test_keypair.json

# Ports frontend/backend (facultatifs si différents)
UI_PORT=5173
API_PORT=4000
WS_PORT=3010
AI_PORT=8000

# Fichier de stratégie active
STRATEGY_PATH=generated/strategy_live.json

# Wallet du créateur pour les commissions (remplace YOUR_SOL_ADDRESS par ton wallet)
CREATOR_WALLET=3X9Tr...YOUR_SOLANA_WALLET_ADDRESS...8zQk

# URL pour les services (utilisé par le frontend)
VITE_API_BASE=http://localhost:4000
VITE_WS_BASE=ws://localhost:3010

# Stripe Configuration (optionnel)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRICE_ID=price_...

# Clés API externes
JUPITER_API_URL=https://quote-api.jup.ag/v6
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com